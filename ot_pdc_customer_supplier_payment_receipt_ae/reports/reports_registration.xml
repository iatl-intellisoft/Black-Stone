        /> -->

        <!-- Register Voucher PDC Receipt Report -->
        <record id="report_voucherpdcreceipt" model="ir.actions.report">
            <field name="name">إيصال استلام دفعه الشيكات</field>
            <field name="model">batch.pdc</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">ot_pdc_customer_supplier_payment_receipt_ae.report_batch_pdc_voucher_receipt</field>
            <field name="report_file">ot_pdc_customer_supplier_payment_receipt_ae.report_batch_pdc_voucher_receipt</field>
            <field name="binding_model_id" ref="model_batch_pdc"/>
            <field name="binding_type">report</field>
        </record>

        <!-- <report id="report_voucherpdcreceipt"
                string="إيصال استلام دفعه الشيكات"
                model="batch.pdc"
                report_type="qweb-pdf"
                file="ot_pdc_customer_supplier_payment_receipt_ae.report_batch_pdc_voucher_receipt"
                name="ot_pdc_customer_supplier_payment_receipt_ae.report_batch_pdc_voucher_receipt"
                menu="True"
        /> -->

        <!-- Register Voucher Payment Report -->
        <record id="report_voucherpayment" model="ir.actions.report">
            <field name="name">إيصال دفع</field>
            <field name="model">account.payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">ot_pdc_customer_supplier_payment_receipt_ae.report_voucher_payment</field>
            <field name="report_file">ot_pdc_customer_supplier_payment_receipt_ae.report_voucher_payment</field>
            <field name="binding_model_id" ref="model_account_payment"/>
            <field name="binding_type">report</field>
        </record>

        <!-- <report id="report_voucherpayment"
