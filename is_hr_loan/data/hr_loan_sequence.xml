<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
        <record id="seq_long_loan" model="ir.sequence">
            <field name="name">Long Loan</field>
            <field name="code">hr.loan.long</field>
            <field name="prefix">Loan NO</field>
            <field name="padding">3</field>
        </record>
        <record id="seq_short_loan" model="ir.sequence">
            <field name="name">Short Loan</field>
            <field name="code">hr.loan.short</field>
            <field name="prefix">Loan NO</field>
            <field name="padding">3</field>
        </record>

        <record id="hr_leave_sell" model="ir.sequence">
            <field name="name">hr.leave.sell</field>
            <field name="code">hr.leave.sell</field>
            <field name="prefix">PUR-TIMEOFF/</field>
            <field name="padding">4</field>
            <field name="active">True</field>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</odoo>