<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Register Voucher Receipt Report -->
        <record id="report_voucherreceipt" model="ir.actions.report">
            <field name="name">إيصال إستلام</field>
            <field name="model">account.payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">ot_pdc_customer_supplier_payment_receipt_ae.report_voucher_receipt</field>
            <field name="report_file">ot_pdc_customer_supplier_payment_receipt_ae.report_voucher_receipt</field>
            <field name="binding_model_id" ref="model_account_payment"/>
            <field name="binding_type">report</field>
        </record>

        <!-- <report id="report_voucherreceipt"
                string="إيصال إستلام"
