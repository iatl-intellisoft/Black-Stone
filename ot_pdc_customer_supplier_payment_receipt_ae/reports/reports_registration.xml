                model="account.payment"
                report_type="qweb-pdf"
                file="ot_pdc_customer_supplier_payment_receipt_ae.report_voucher_payment"
                name="ot_pdc_customer_supplier_payment_receipt_ae.report_voucher_payment"
                menu="True"
        /> -->

        <!-- Register print check report -->
        <record id="report_printcheck" model="ir.actions.report">
            <field name="name">طباعه الشيك</field>
            <field name="model">account.payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">ot_pdc_customer_supplier_payment_receipt_ae.report_check</field>
            <field name="report_file">ot_pdc_customer_supplier_payment_receipt_ae.report_check</field>
            <field name="binding_model_id" ref="model_account_payment"/>
            <field name="binding_type">report</field>
        </record>

        <!-- <report
                id="report_printcheck"
                string="طباعه الشيك"
                model="account.payment"
                report_type="qweb-pdf"
                name="ot_pdc_customer_supplier_payment_receipt_ae.report_check"
                file="ot_pdc_customer_supplier_payment_receipt_ae.report_check"
        /> -->

        <!-- check paper format -->
        <record id="paperformat_check" model="report.paperformat">
            <field name="name">Sudan Check</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="format">custom</field>
            <field name="page_height">90</field>
            <field name="page_width">250</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">1</field>
            <field name="margin_bottom">1</field>
            <field name="margin_left">3</field>
            <field name="margin_right">3</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">3</field>
            <field name="dpi">80</field>
            <field name="report_ids"
                   eval="[(4, ref('ot_pdc_customer_supplier_payment_receipt_ae.report_printcheck'))]"/>
        </record>
    </data>
